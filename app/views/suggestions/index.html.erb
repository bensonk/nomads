
<% if @my_suggestions and @my_suggestions.length > 0 %>
  <div class="listing_suggestions">
  <h2 class="scaffold">Suggestions</h2>
  <table class="zebra">
    <tr>
      <th>User</th>
      <th>Title</th>
      <th>Type</th>
      <th>Content</th>
      <th></th>
    </tr>
    <%= render :partial => 'suggestion_row', :collection => @my_suggestions %>
    <%= render :partial => 'suggestion_row', :collection => (@suggestions - @my_suggestions) %>
  </table>
  </div>
<% end %>
