<%= stylesheet_link_tag 'fancy_black' %> 
<div id="container">
  <div id="photo_archive_container">
    <% @photos.each do |p| %>
      <div class="archive_photo">
        <%= link_to(image_tag(p.url('m'), :title => p.title, :border => 0, :alt => p.title), p.url('b'), :class => "fancy", :rel =>"group") %>
      </div>
    <% end %>
  </div>
</div>
